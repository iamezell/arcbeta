{
  "name": "arcbeta",
  "version": "1.0.0",
  "description": "Multiplayer WebXR First-Person Theatre Prototype",
  "main": "backend/https-server.js",
  "type": "commonjs",
  "scripts": {
    "dev:backend": "nodemon --watch backend --exec ts-node backend/https-server.ts",
    "dev:frontend": "vite",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "vite build",
    "build": "npm run build:backend && npm run build:frontend",
    "start": "node dist/backend/https-server.js"
  },
  "keywords": ["webxr", "threejs", "socketio", "multiplayer"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "vite": "^5.0.10",
    "@types/three": "^0.160.0",
    "three": "^0.160.0",
    "socket.io-client": "^4.6.1"
  }
}

